{
    "name": "hello-world",
    "version": "1.2.5",
    "description": "Example hello world function using azure-middy",
    "main": "src/functions/*.*s",
    "scripts": {
        "build": "tsc && cpy typings*.d.ts dist && cpy src/functions/*.cjs dist",
        "build:watch": "tsc --watch",
        "start": "npx func start",
        "test": "echo \"Error: no test specified\" && exit 1",
        "type-check": "tsc --noEmit"
    },
    "files": [
        "status.cjs",
        "helloMiddy.ts",
        "tsconfig.json",
        "helloWorld.cjs"
    ],
    "keywords": [
        "example",
        "hello",
        "functions",
        "faas",
        "serverless",
        "azure",
        "cloud",
        "javascript",
        "nodejs"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kevboutin/azure-middy.git",
        "directory": "examples/hello-world"
    },
    "bugs": {
        "url": "https://github.com/kevboutin/azure-middy/issues"
    },
    "author": "Azure-Middy contributors (https://github.com/kevboutin/azure-middy/graphs/contributors)",
    "license": "MIT",
    "homepage": "https://github.com/kevboutin/azure-middy#readme",
    "dependencies": {
        "@azure/functions": "^4.8.0",
        "@kevboutin/azure-middy-core": "^0.2.14",
        "@kevboutin/azure-middy-logger": "^0.2.14",
        "@kevboutin/azure-middy-types": "^0.1.2",
        "cpy-cli": "^6.0.0"
    },
    "devDependencies": {
        "azure-functions-core-tools": "^4.2.2"
    }
}
