{
    "name": "@kevboutin/azure-middy-core",
    "version": "0.2.12",
    "description": "A Node.js middleware engine for Azure functions (core package)",
    "engines": {
        "node": ">=18"
    },
    "engineStrict": true,
    "publishConfig": {
        "access": "public"
    },
    "main": "./dist/index.js",
    "types": "./dist/typings.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js",
            "types": "./dist/typings.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "type": "commonjs",
    "scripts": {
        "build": "tsc && cpy typings*.d.ts dist",
        "build:watch": "tsc --watch",
        "test": "npm run test:unit",
        "test:unit": "ava",
        "type-check": "tsc --noEmit"
    },
    "license": "MIT",
    "keywords": [
        "azure",
        "middleware",
        "serverless",
        "framework",
        "function",
        "app",
        "faas",
        "function"
    ],
    "author": {
        "name": "Azure-middy contributors",
        "url": "https://github.com/kevboutin/azure-middy/graphs/contributors"
    },
    "repository": {
        "type": "git",
        "url": "github:kevboutin/azure-middy",
        "directory": "packages/azure-middy-core"
    },
    "bugs": {
        "url": "https://github.com/kevboutin/middy/issues"
    },
    "devDependencies": {
        "@types/node": "^24.3.1",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@azure/functions": "^4.8.0",
        "@kevboutin/azure-middy-types": "^0.1.0",
        "cpy-cli": "^6.0.0"
    },
    "include": [
        "**/*.ts",
        "typings.d.ts"
    ]
}
