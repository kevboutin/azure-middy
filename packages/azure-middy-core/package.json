{
    "name": "@kevboutin/azure-middy-core",
    "version": "0.2.6",
    "description": "A Node.js middleware engine for Azure functions (core package)",
    "engines": {
        "node": ">=18"
    },
    "engineStrict": true,
    "publishConfig": {
        "access": "public"
    },
    "main": "./index.cjs",
    "exports": {
        "require": "./index.cjs",
        "import": "./index.ts",
        "types": "./typings.d.ts",
        "default": "./index.ts"
    },
    "files": [
        "index.cjs",
        "index.ts",
        "typings.d.ts",
        "tsconfig.json",
        "example.ts"
    ],
    "type": "commonjs",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "test": "npm run test:unit",
        "test:unit": "ava",
        "type-check": "tsc --noEmit"
    },
    "license": "MIT",
    "keywords": [
        "azure",
        "middleware",
        "serverless",
        "framework",
        "function",
        "app",
        "faas",
        "function"
    ],
    "author": {
        "name": "Azure-middy contributors",
        "url": "https://github.com/kevboutin/azure-middy/graphs/contributors"
    },
    "repository": {
        "type": "git",
        "url": "github:kevboutin/azure-middy",
        "directory": "packages/azure-middy-core"
    },
    "bugs": {
        "url": "https://github.com/kevboutin/middy/issues"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@azure/functions": "^4.7.2-preview"
    }
}
