{
    "name": "@kevboutin/azure-middy-mongodb",
    "version": "0.2.8",
    "description": "MongoDB middleware for the azure-middy framework",
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        "require": "./index.cjs",
        "import": "./index.ts",
        "types": "./typings.d.ts",
        "default": "./index.ts"
    },
    "files": [
        "index.cjs",
        "index.ts",
        "typings.d.ts",
        "tsconfig.json",
        "example.ts"
    ],
    "type": "commonjs",
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "test": "npm run test:unit",
        "test:unit": "ava --config test.config.js",
        "type-check": "tsc --noEmit"
    },
    "license": "MIT",
    "keywords": [
        "azure",
        "middleware",
        "serverless",
        "framework",
        "function",
        "app",
        "faas",
        "function"
    ],
    "author": "Azure-Middy contributors (https://github.com/kevboutin/azure-middy/graphs/contributors)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kevboutin/azure-middy.git",
        "directory": "packages/azure-middy-keyvault-secrets"
    },
    "bugs": {
        "url": "https://github.com/kevboutin/azure-middy/issues"
    },
    "dependencies": {
        "mongoose": "^8.15.2"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0"
    },
    "homepage": "https://github.com/kevboutin/azure-middy#readme",
    "main": "index.cjs",
    "types": "./typings.d.ts"
}
