{
    "name": "@kevboutin/azure-middy-mongodb",
    "version": "0.2.14",
    "description": "MongoDB middleware for the azure-middy framework",
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "./dist/index.js",
    "types": "./dist/typings.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js",
            "types": "./dist/typings.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "type": "commonjs",
    "scripts": {
        "build": "tsc && cpy typings*.d.ts dist",
        "build:watch": "tsc --watch",
        "test": "npm run test:unit",
        "test:unit": "ava --config test.config.js",
        "type-check": "tsc --noEmit"
    },
    "license": "MIT",
    "keywords": [
        "azure",
        "middleware",
        "serverless",
        "framework",
        "function",
        "app",
        "faas",
        "function"
    ],
    "author": "Azure-Middy contributors (https://github.com/kevboutin/azure-middy/graphs/contributors)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kevboutin/azure-middy.git",
        "directory": "packages/azure-middy-keyvault-secrets"
    },
    "bugs": {
        "url": "https://github.com/kevboutin/azure-middy/issues"
    },
    "dependencies": {
        "@kevboutin/azure-middy-types": "^0.1.2",
        "cpy-cli": "^6.0.0",
        "mongoose": "^8.15.2"
    },
    "devDependencies": {
        "@types/node": "^24.3.1",
        "typescript": "^5.0.0"
    },
    "include": [
        "**/*.ts",
        "typings.d.ts"
    ],
    "homepage": "https://github.com/kevboutin/azure-middy#readme"
}
