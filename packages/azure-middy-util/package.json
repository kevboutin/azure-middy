{
    "name": "@kevboutin/azure-middy-util",
    "version": "0.2.13",
    "description": "Node.js middleware engine for Azure functions (azure-middy-util package)",
    "main": "./index.js",
    "types": "./typings.d.ts",
    "type": "commonjs",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.js",
            "types": "./dist/typings.d.ts"
        }
    },
    "scripts": {
        "build": "tsc && cpy typings*.d.ts dist",
        "build:watch": "tsc --watch",
        "test": "npm run test:unit",
        "test:unit": "ava",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "azure",
        "middleware",
        "serverless",
        "framework",
        "function",
        "app",
        "faas",
        "function"
    ],
    "author": {
        "name": "Azure-Middy contributors",
        "url": "https://github.com/kevboutin/azure-middy/graphs/contributors"
    },
    "repository": {
        "type": "git",
        "url": "github:kevboutin/azure-middy",
        "directory": "packages/azure-middy-util"
    },
    "bugs": {
        "url": "https://github.com/kevboutin/azure-middy/issues"
    },
    "files": [
        "dist"
    ],
    "include": [
        "**/*.ts",
        "typings.d.ts"
    ],
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^24.3.1",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@kevboutin/azure-middy-types": "^0.1.1",
        "cpy-cli": "^6.0.0"
    }
}
